<form id="{{ formId }}" class="form">
    <div class="form__title">{{ formTitle }}</div>

    <div class="form__items">
        {{#each fields}}
            {{> input/input}}
        {{/each}}
    </div>

    <div class="form__buttons">
    {{#each buttons}}
        {{> button/button}}
    {{/each}}
    </div>
</form>